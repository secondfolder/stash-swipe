{
  "name": "stash-tv",
  "version": "0.1.2",
  "description": "TikTok for Stash.",
  "main": "index.js",
  "scripts": {
    "app:build": "vite build src/ --mode development --sourcemap true --emptyOutDir --base './' --outDir ../dist/app --minify false",
    "app:serve": "webpack serve --config webpack/app.dev.js",
    "build": "npm run app:build && npm run plugin:build",
    "plugin:build": "webpack --config webpack/plugin.config.js",
    "storybook": "storybook dev -p 6006",
    "stash-ui:build": "cd vendor/stash/ui/v2.5 && yarn install && npm run gqlgen && ./node_modules/.bin/tsc --outDir ./build --noEmit false --declaration true --rootDir . && npx copyfiles \"src/**/*.css\" \"src/**/*.scss\" build && npx -y tsc-alias --outDir build && find node_modules -depth -mindepth 1 ! -path \"node_modules/bootstrap\" ! -path \"node_modules/bootstrap/scss\" ! -path \"node_modules/bootstrap/scss/*\" -print0 | xargs -0 -n 20 rm -rf",
    "stash-ui:update-repo": "echo git subtree pull --prefix=vendor/stash https://github.com/stashapp/stash.git \"$(npm run -s stash-ui:get-latest-tag)\" --squash",
    "stash-ui:get-latest-tag": "node -e \"fetch('https://api.github.com/repos/stashapp/stash/releases/latest').then(res=>res.json()).then(data=>console.log(data.tag_name))\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secondfolder/stash-tv.git"
  },
  "author": "secondfolder",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/secondfolder/stash-tv/issues"
  },
  "homepage": "https://github.com/secondfolder/stash-tv#readme",
  "dependencies": {
    "@apollo/client": "3.8.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "classnames": "^2.5.1",
    "iso-639-1": "^3.1.2",
    "json-to-graphql-query": "^2.2.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scrubber": "^2.0.0",
    "react-select": "^5.8.0",
    "react-transition-group": "^4.4.5",
    "vite": "^7.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.7.0",
    "@silvermine/videojs-airplay": "^1.2.0",
    "@silvermine/videojs-chromecast": "^1.4.1",
    "@storybook/addon-a11y": "8.2.9",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-webpack5": "8.2.9",
    "@storybook/test": "8.2.9",
    "@types/apollo-upload-client": "18.0.0",
    "@types/react": "17.0.53",
    "@types/react-dom": "17.0.19",
    "@types/react-transition-group": "^4.4.10",
    "@types/video.js": "^7.3.58",
    "apollo-upload-client": "^18.0.1",
    "bootstrap": "^4.6.2",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-js": "^4.2.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^8.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "i18n-iso-countries": "^7.5.0",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.0",
    "react-bootstrap": "^1.6.6",
    "react-intl": "^6.2.8",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "storybook": "^8.2.9",
    "style-loader": "^4.0.0",
    "thehandy": "^1.0.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "video.js": "^7.21.3",
    "videojs-abloop": "^1.2.0",
    "videojs-contrib-dash": "^5.1.1",
    "videojs-mobile-ui": "^0.8.0",
    "videojs-seek-buttons": "^3.0.1",
    "videojs-vr": "1.8.0",
    "videojs-vtt.js": "^0.15.4",
    "webpack": "^5.93.0",
